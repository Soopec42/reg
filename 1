#include <iostream>
#include <vector>
#include <algorithm>
#include <string>


using namespace std;



int main()
{
	
	long long a, b, k, m, x = -1, y = 0, t;
	cin >> t;
	for (int i = 0; i < t; i++)
	{
		cin >> a >> b >> k >> m;


		for (long long i = 0; i <= k; i++)
		{
			if (m % (i + 1) == 0)
			{
				if (a > i && b > ((m / (i + 1)) - 1))
				{
					x = i;
					cout << i << " ";
					cout << (m / (i + 1)) - 1 << endl;
					break;
				}

			}

		}
		if (x == -1)
			cout << -1 << endl;
		x = -1;
	}
}
